function DNAStrand(dna) {
    let res = '';

    for (let i = 0; i < dna.length; i++) {
        if (dna[i] === 'A') {
            res += dna[i] = 'T'
        } else if (dna[i] === 'T') {
            res += dna[i] = 'A'
        } else if (dna[i] === 'C') {
            res += dna[i] = 'G'
        } else if (dna[i] === 'G') {
            res += dna[i] = 'C'
        } else {
            res += dna[i]
        }
    }
    return res
}
