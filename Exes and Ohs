function XO(str) {
    let x = 0;
    let o = 0;

    for (let i = 0; i < str.length; i++) {
        if (str[i].toLowerCase().includes('x')) {
            x++;
        } else if (str[i].toLowerCase().includes('o')) {
            o++;
        }
    }

    if (x === o) {
        return true
    } else {
        return false
    }
}
